<div class="member member-constructor">
    <div class="member-constructor-signature">
        <h4><%- ViewHelper.getMethodTitleHtml( _self, false ) %></h4>
    </div>
    <div class="member-constructor-desc">

        <div class="member-description">
            <%- desc %>
        </div>

        <%if ( _self.hasOwnProperty('remind') ) {%>
        <!-- remind start -->
        <%for ( var i=0, _remind=null; _remind=remind[i]; i++ ) {%>
        <ul class="member-remind">
            <li class="alert alert-success">
                提示： <%- _remind %>
            </li>
        </ul>
        <%}%>
        <!-- remind end -->
        <%}%>

        <%if ( _self.hasOwnProperty('warning') ) {%>
        <!-- warning start -->
        <%for ( var i=0, _warning=null; _warning=warning[i]; i++ ) {%>
        <ul class="member-warning">
            <li class="alert alert-danger">
                警告： <%- _warning %>
            </li>
        </ul>
        <%}%>
        <!-- warning end -->
        <%}%>

    </div>
    <div class="member-constructor-info">

        <%if ( _self.hasOwnProperty( 'param' ) ) {%>
        <!-- param start -->
        <fileset>
            <legend>参数列表</legend>
            <table class="table">
                <thead>
                    <tr>
                        <th>参数名</th>
                        <th>类型</th>
                        <th>描述</th>
                    </tr>
                </thead>
                <tbody>
                    <%for (var i=0, para=null; para=param[i]; i++) {%>
                    <tr>
                        <td>
                            <%- para.name %>
                        </td>
                        <td>
                            <a href="#<%- para.type %>"><%- para.type %></a>
                        </td>
                        <td>
                            <%- para.desc %>
                        </td>
                    </tr>
                    <%}%>
                </tbody>
            </table>
        </fileset>
        <!-- param end -->
        <%}%>

        <%if ( _self.hasOwnProperty( 'example' ) ) {%>
        <!-- example start -->
        <fileset>
            <legend>示例</legend>
            <%for ( var i=0, exp=null; exp = example[i]; i++ ) {%>
                <div class="example">
                    <%- exp %>
                </div>
            <%}%>
        </fileset>
        <!-- example end -->
        <%}%>
    </div>
</div>