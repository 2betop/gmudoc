<div class="member member-property">
    <div class="member-property-signature">
        <h4><%- ViewHelper.getPropertyTitleHtml( _self, false ) %></h4>
        <div class="member-flag">
        <%if ( _self.hasOwnProperty( 'static' ) ) {%>
            <span class="label label-primary">静态</span>
        <%}%>
        </div>
    </div>

    <div class="member-property-desc">

        <div class="member-description">
            <%- desc %>
        </div>

        <%if ( _self.hasOwnProperty('remind') ) {%>
        <!-- remind start -->
        <%for ( var i=0, _remind=null; _remind=remind[i]; i++ ) {%>
        <ul class="member-remind">
            <li class="alert alert-success">
                提示： <%- _remind %>
            </li>
        </ul>
        <%}%>
        <!-- remind end -->
        <%}%>

        <%if ( _self.hasOwnProperty('warning') ) {%>
        <!-- warning start -->
        <%for ( var i=0, _warning=null; _warning=warning[i]; i++ ) {%>
        <ul class="member-warning">
            <li class="alert alert-danger">
                警告： <%- _warning %>
            </li>
        </ul>
        <%}%>
        <!-- warning end -->
        <%}%>

    </div>

    <div class="member-property-info">

        <table class="table">
            <thead>
                <tr>
                    <th>类型</th>
                    <th>默认值</th>
                    <th>描述</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        <%- type %>
                    </td>
                    <td>
                        <%if ( !_self.hasOwnProperty( 'default' ) ) {%>
                            无
                        <%} else {%>
                            <%- _self['default'] %>
                        <%}%>
                    </td>
                    <td>
                        <%- desc %>
                    </td>
                </tr>
            </tbody>
        </table>

        <%if ( _self.hasOwnProperty( 'example' ) ) {%>
        <!-- example start -->
        <fileset>
            <legend>示例</legend>
            <%for ( var i=0, exp=null; exp = example[i]; i++ ) {%>
                <div class="example">
                    <%- exp %>
                </div>
            <%}%>
        </fileset>
        <!-- example end -->
        <%}%>

    </div>
</div>