<div class="member-clazz">
    <div class="member-clazz-name">
        <h3><%- ViewHelper.getClassTitleHtml( _self, false ) %></h3>
    </div>

    <div class="member-clazz-desc">

        <div class="member-description">
            <%- desc %>
        </div>

        <%if ( _self.hasOwnProperty('remind') ) {%>
        <!-- remind start -->
        <%for ( var i=0, _remind=null; _remind=remind[i]; i++ ) {%>
        <ul class="member-remind">
            <li>
                <%- _remind %>
            </li>
        </ul>
        <%}%>
        <!-- remind end -->
        <%}%>

        <%if ( _self.hasOwnProperty('warning') ) {%>
        <!-- warning start -->
        <%for ( var i=0, _warning=null; _warning=warning[i]; i++ ) {%>
        <ul class="member-warning">
            <li>
                <%- _warning %>
            </li>
        </ul>
        <%}%>
        <!-- warning end -->
        <%}%>

    </div>

    <div class="member-box">
        <div class="member-clazz-info">

            <%- ViewHelper.getPropertyWidget( _self ) %>

            <%- ViewHelper.getEventWidget( _self ) %>

            <%- ViewHelper.getConstructorWidget( _self ) %>

            <%- ViewHelper.getMethodWidget( _self ) %>

        </div>

        <div class="clazz-member-list">

            <%if (_self.hasOwnProperty('__member')) {%>
            <fileset>
                <legend>类成员详细描述(<%- ViewHelper.getClassTitleHtml( _self, true ) %>)</legend>

                <div class="member-box">
                    <%if ( __memberTpl.hasOwnProperty("property") ) {%>
                    <%for ( var i=0, _member=null; _member=__memberTpl.property[i]; i++ ) { %>
                        <%- _member %>
                    <%}%>
                    <%}%>

                    <%if ( __memberTpl.hasOwnProperty("event") ) {%>
                    <%for ( var i=0, _member=null; _member=__memberTpl.event[i]; i++ ) { %>
                        <%- _member %>
                    <%}%>
                    <%}%>

                    <%if ( __memberTpl.hasOwnProperty("constructor") ) {%>
                    <%for ( var i=0, _member=null; _member=__memberTpl.constructor[i]; i++ ) { %>
                        <%- _member %>
                    <%}%>
                    <%}%>

                    <%if ( __memberTpl.hasOwnProperty("method") ) {%>
                    <%for ( var i=0, _member=null; _member=__memberTpl.method[i]; i++ ) { %>
                        <%- _member %>
                    <%}%>
                    <%}%>
                </div>
            </fileset>
            <%}%>

        </div>
    </div>

</div>