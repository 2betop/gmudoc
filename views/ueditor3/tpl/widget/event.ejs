<!-- event list start -->
<fileset>
    <legend>事件列表</legend>
    <table class="table">
        <thead>
            <tr>
                <th>事件名</th>
                <th>描述</th>
            </tr>
        </thead>
        <tbody>
        <%for (var i=0, _evt=null; _evt=events[i]; i++) {%>
            <tr>
                <td>
                    <%- ViewHelper.getEventTitleHtml( _evt, true ) %>
                </td>
                <td>
                    <%- _evt.desc %>
                </td>
            </tr>
        <%}%>
        </tbody>
    </table>
</fileset>
<!-- event list end -->